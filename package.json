  "license": "MIT",
  "scripts": {
    "sapper:dev": "NODE_ENV=development sapper dev --ext '.svelte .svx'",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
    "dev": "run-p validate:dev lint:fix sapper:dev",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "prod": "NODE_ENV=production sapper build --legacy --ext '.svelte .svx' && node __sapper__/build",
    "clean": "rm -rf ./__sapper__",
    "export": "NODE_ENV=production sapper export --legacy --ext '.svelte .svx'",
    "deploy": "npm run clean && npm run export && node ./src/helpers/deploy.js"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "gh-pages": "^3.1.0",
    "helmet": "^4.1.0",
    "polka": "^0.5.2",
    "sirv": "^1.0.6",
    "uuid": "^8.3.0"
  },
