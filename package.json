{
  "name": "markushatvan.com",
  "description": "Markus Hatvan Website",
  "author": "Markus Hatvan <contact@markushatvan.com>",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "run-p sapper:dev validate:dev lint:fix watch:css",
    "sapper:dev": "sapper dev --ext '.svelte .svx'",
    "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "yarn lint --fix",
    "watch:css": "NODE_ENV=development postcss src/global.pcss -o static/global.css -w",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "build": "run-s build:css build:prod",
    "export": "run-s build:css build:export",
    "build:css": "NODE_ENV=production postcss src/global.pcss -o static/global.css",
    "build:prod": "sapper build --legacy --ext '.svelte .svx' && node __sapper__/build",
    "build:export": "sapper export --legacy --ext '.svelte .svx'"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "helmet": "^4.3.1",
    "polka": "^0.5.2",
    "sirv": "^1.0.10",
    "uuid": "^8.3.2"
  },
