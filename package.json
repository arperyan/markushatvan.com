    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "prod": "NODE_ENV=production sapper build --legacy --ext '.svelte .svx' && node __sapper__/build",
    "export": "NODE_ENV=production sapper export --legacy --ext '.svelte .svx'"
  },
